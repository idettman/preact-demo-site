{
  "name": "idettman.github.io",
  "version": "1.0.0",
  "description": "idettman's github home",
  "main": "index.js",
  "engines": {
    "npm": ">=4.2.0",
    "node": ">=7.9.0"
  },
  "scripts": {
    "build": "npm run postcss-dev",
    "build-dev": "npm run build-js-dev | npm run build-postcss-dev",
    "build-prod": "npm run build-js-prod | npm run build-postcss-prod",
    "build-js-dev": "NODE_ENV=development rollup --config build/rollup.config.js",
    "build-js-prod": "NODE_ENV=production rollup --config build/rollup.config.js",
    "build-postcss-dev": "NODE_ENV=development node build/postcss.runner.js css/input.css -o style.css -c build/postcss.config.js",
    "build-postcss-prod": "NODE_ENV=production node build/postcss.runner.js css/input.css -o style.css -c build/postcss.config.js",
    "rollup": "rollup",
    "serve": "node build/http-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idettman/idettman.github.io.git"
  },
  "author": {
    "name": "idettman",
    "url": "https://github.com/idettman"
  },
  "license": "MIT",
  "keywords": [
    "BublÃ©",
    "Preact",
    "Rollup",
    "StandardJS"
  ],
  "bugs": {
    "url": "https://github.com/idettman/idettman.github.io/issues"
  },
  "homepage": "https://github.com/idettman/idettman.github.io#readme",
  "standard": {
    "ignore": [
      "build"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "preact": "^8.1.0",
    "preact-router": "^2.4.5",
    "preact-async-route": "^1.0.3"
  },
  "devDependencies": {
    "chalk": "latest",
    "chokidar": "^1.6.1",
    "cssnano": "^3.10.0",
    "dependency-graph": "^0.5.0",
    "fs-promise": "^2.0.2",
    "get-stdin": "^5.0.1",
    "globby": "^6.1.0",
    "ora": "^1.2.0",
    "postcss": "latest",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-load-config": "^1.2.0",
    "postcss-modules": "^0.7.0",
    "postcss-reporter": "^3.0.0",
    "read-cache": "^1.0.0",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.2",
    "yargs": "latest"
  }
}
